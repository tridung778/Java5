<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
	"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	<definition name="layout" template="/WEB-INF/views/layout/index.jsp">
		<put-attribute name="header" value="/WEB-INF/views/layout/_header.jsp" />
		<put-attribute name="body" value="" />
		<put-attribute name="aside" value="/WEB-INF/views/layout/_aside.jsp" />
		<put-attribute name="footer" value="/WEB-INF/views/layout/_footer.jsp" />
		
	</definition>
	<definition name="*/*" extends="layout">
		<put-attribute name="body" value="/WEB-INF/views/{1}/{2}.jsp" />
	</definition>
</tiles-definitions>